//brute force apporoch
		    int [] arr = {1,2,4,5,6};
		    for(int i=0;i<arr.length;i++)
		    {
		    	int flag =0;
		    	for(int j=0;j<arr.length-1;j++)
		    	{
		    		if(arr[j]==i)
		    		{
		    			flag=1;
		    			break;
		    		}
		    	}
		    	if(flag==0)
		    	{
		    		System.out.println("Missing Value : "+i);
		    	}
		    }


// Better solution
	        int[] arr = {1, 2, 4, 5, 6};  // 3 is missing
	        int n = 6; // Total numbers from 1 to 6

	        int totalSum = n * (n + 1) / 2; // Sum of first n natural numbers
	        int actualSum = 0;

	        for (int num : arr) {
	            actualSum += num;
	        }

	        int missing = totalSum - actualSum;
	        System.out.println("Missing number is: " + missing);
	        System.out.println("Sum of present elements: " + actualSum);



//Optimal solution
class Solution {
    public int missingNumber(int[] nums) {
        int xor1 =0;
        int xor2 =0;
        int n =nums.length;
        for(int i=0;i<n;i++)
        {
            xor2 = xor2 ^ nums[i];
            xor1 = xor1 ^ i;
        }
        xor1 = xor1 ^ n;
        return (xor1^xor2);
    }
}    
